"unlittwotexture"
{
	"$baseTexture" "models\cp_hadal/rays_base"
	"$texture2" "models\cp_hadal/rays_passthrough"
	"$model" "1"
	"$surfaceprop" "nodecal"
	"$additive" "1"
	
	"$texoffset"	"[0 0]"
	"$texoffset2"	"[0 0]"
	"$texscale" "1"
	
	"Proxies"
	{	
		"LinearRamp"
		{
			"initialValue" 0
			"rate" 0.03
			"resultVar"	"$texoffset[0]"
		}
		"TextureTransform"
		{
			"scaleVar" "$texscale"
			"translateVar" "$texoffset"
			"resultVar" "$texture2transform"
		}
		"LinearRamp"
		{
			"initialValue" 0
			"rate" -0.01
			"resultVar"	"$texoffset2[1]"
		}
		"TextureTransform"
		{
			"scaleVar" "$texscale"
			"translateVar" "$texoffset2"
			"resultVar" "$basetexturetransform"
		}
	}
}